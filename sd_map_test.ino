#define TILE_SIZE 10
const uint8_t map_tiles[] PROGMEM = {
    8,8,
B00000000,
B00000000,
B00000000,
B00000000,
B00000000,
B00000000,
B00000000,
B00000000,
8,8,
B10101010,
B01010101,
B10101010,
B01010101,
B10101010,
B01010101,
B10101010,
B01010101,
};

#define MAP_BUFFER_SIZE 16
uint8_t map_buffer[16][16];

uint8_t sd_buffer[MAP_BUFFER_SIZE];//Space for up to a full row

int16_t cameraX = 0;
int16_t cameraY = 0;

#define FILE_OPENED 0
#define FILE_ERROR 1
uint8_t file_status = FILE_ERROR;

